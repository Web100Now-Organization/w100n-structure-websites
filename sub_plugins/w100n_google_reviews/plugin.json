{
  "id": "w100n_structure_websites_google_reviews",
  "name": "Google Reviews",
  "version": "1.2.0",
  "description": "Plugin for displaying Google Reviews on the website.",
  "author": "Web100Now",
  "organization": "web100now",
  "short_name": "google_reviews",
  "category": "reviews",
  "default_enabled": false,
  "requires_role": "website",
  "write_requires_role": "platform",
  "sync_fields": ["short_name", "name", "enabled"],
  "permissions": {
    "read": ["bot", "website", "admin", "platform"],
    "write": ["platform"]
  },
  "graphql_schema": "schema.graphqls",
  "resolvers": "resolvers.go",
  "endpoints": [
    {
      "type": "query",
      "name": "googleReviews",
      "description": "Fetch reviews from Google My Business",
      "requires_write": false
    }
  ],
  "dependencies": [],
  "config": {
    "mongo_collections": ["google_reviews"]
  }
}

